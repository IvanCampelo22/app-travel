{
  "executionRoleArn": "arn:aws:iam::060454264788:role/v10-eng-ecsTaskExecutionRole-dev",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/v10-eng-dev-backend-python",
          "awslogs-region": "sa-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 8000,
          "protocol": "tcp",
          "containerPort": 8000
        }
      ],
      "linuxParameters": {
        "initProcessEnabled": true
      },
      "image": "060454264788.dkr.ecr.sa-east-1.amazonaws.com/v10-eng-dev-backend-python:latest",
      "name": "backend-python",
      "secrets": [
        {
          "name": "DB_ENGINE",
          "valueFrom": "arn:aws:ssm:sa-east-1:060454264788:parameter/dev/backend/DB_ENGINE"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "arn:aws:ssm:sa-east-1:060454264788:parameter/dev/backend/DB_NAME"
        },
        {
          "name": "DB_USER",
          "valueFrom": "arn:aws:ssm:sa-east-1:060454264788:parameter/dev/backend/DB_USER"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:ssm:sa-east-1:060454264788:parameter/dev/backend/DB_PASSWORD"
        },
        {
          "name": "DB_HOST",
          "valueFrom": "arn:aws:ssm:sa-east-1:060454264788:parameter/dev/backend/DB_HOST"
        },
        {
          "name": "DB_PORT",
          "valueFrom": "arn:aws:ssm:sa-east-1:060454264788:parameter/dev/backend/DB_PORT"
        }
      ]
    }
  ],
  "placementConstraints": [],
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::060454264788:role/v10-eng-ecsTaskRole-dev",
  "family": "v10-eng-dev-backend-python",
  "requiresAttributes": [],
  "requiresCompatibilities": ["EC2"],
  "networkMode": "awsvpc",
  "cpu": "512"
}
