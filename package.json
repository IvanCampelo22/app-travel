{
  "name": "viagem10-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "commit": "cz",
    "prepare": "husky install",
    "lint": "nx run-many --target=lint",
    "build": "nx run-many --target=build",
    "start": "NODE_ENV=development docker-compose up -d && nx run-many --target=serve",
    "test": "NODE_ENV=test docker-compose up -d && nx run-many --target=test --parallel=5 && yarn db:drop",
    "db:generate": "NODE_ENV=development docker-compose up -d && prisma generate --schema=libs/server/infra/database/prisma/schema.prisma",
    "db:migrate": "docker-compose up db -d && prisma migrate dev --schema=libs/server/infra/database/prisma/schema.prisma",
    "db:deploy": "docker-compose up db -d && prisma migrate deploy --schema=libs/server/infra/database/prisma/schema.prisma",
    "db:push": "prisma db push --schema=libs/server/infra/database/prisma/schema.prisma",
    "db:drop": "prisma db execute --file ./drop-schemas.sql --schema=libs/server/infra/database/prisma/schema.prisma",
    "db:push:schema": "docker-compose up db -d && prisma db push --schema=libs/server/infra/database/prisma/schema.prisma --skip-generate",
    "db:reset": "docker-compose up db -d && prisma migrate reset --schema=libs/server/infra/database/prisma/schema.prisma",
    "db:studio": "docker-compose up db -d && npx prisma studio --schema=libs/server/infra/database/prisma/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.5",
