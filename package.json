{
  "name": "viagem10-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "commit": "cz",
    "prepare": "husky install",
    "lint": "nx run-many --target=lint",
    "build": "nx run-many --target=build",
    "start": "NODE_ENV=development docker-compose up -d && nx run-many --target=serve",
    "test": "NODE_ENV=test docker-compose up -d && nx run-many --target=test",
    "db:generate": "NODE_ENV=development docker-compose up -d && prisma generate --schema=libs/server/infra/database/prisma/schema.prisma",
    "db:migrate": "docker-compose up database -d && prisma migrate dev --schema=libs/server/infra/database/prisma/schema.prisma",
    "db:reset": "docker-compose up database -d && prisma migrate reset --schema=libs/server/infra/database/prisma/schema.prisma",
    "db:studio": "docker-compose up database -d && npx prisma studio --schema=libs/server/infra/database/prisma/schema.prisma"
  },
  "private": true,
